{
  "name": "bfx-cli",
  "version": "1.0.0",
  "description": "Bitfinex CLI Trading Interface",
  "engines": {
    "node": ">=7"
  },
  "bin": {
    "bfx-cli": "index.js"
  },
  "main": "index.js",
  "husky": {
    "hooks": {
      "pre-commit": "npm test"
    }
  },
  "scripts": {
    "lint": "standard",
    "test": "npm run lint && npm run unit",
    "unit": "NODE_ENV=test nyc --reporter=lcov mocha -b --recursive"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/f3rno/bfx-cli.git"
  },
  "keywords": [
    "trading",
    "cli",
    "bitfinex"
  ],
  "author": "Cris Mihalache <cris.m@bitfinex.com> (https://www.bitfinex.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/f3rno/bfx-cli/issues"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "husky": "^4.2.3",
    "mocha": "^7.1.0",
    "nyc": "^15.0.0",
    "standard": "^14.3.1"
  },
  "dependencies": {
    "aes-js": "^3.1.2",
    "bfx-api-node-models": "^1.1.8",
    "bfx-hf-util": "^1.0.5",
    "bitfinex-api-node": "^4.0.10",
    "cli-table2": "^0.2.0",
    "colors": "^1.4.0",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "enquirer": "^2.3.4",
    "p-iteration": "^1.1.8",
    "pipe-args": "^1.3.0",
    "print-message": "^3.0.1",
    "scrypt-js": "^2.0.4",
    "sinon": "^9.0.0",
    "socks-proxy-agent": "^5.0.0",
    "sprintf-js": "^1.1.2",
    "yargs": "^15.1.0"
  }
}
